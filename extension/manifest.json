{
    "name": "ProSnitch",
    "version": "1.0",
    "description": "Chrome Extension to reveal progamer aliases in the starlog.gg Leaderboard",
    "manifest_version": 2,
    "permissions": [
        "https://liquipedia.net/starcraft/StarCraft_Remastered_Ladder"
    ],
    "content_scripts": [
        {
            "matches": ["https://starlog.gg/en/leaderboard*"],
            "js": ["snitch.js"]
        }
    ]
}
