{
    "name": "ProSnitch",
    "version": "1.0",
    "description": "Chrome Extension to reveal progamer aliases in the starlog.gg Leaderboard",
    "manifest_version": 2,
    "permissions": [
        "https://liquipedia.net/starcraft/StarCraft_Remastered_Ladder"
    ],
    "content_scripts": [
        {
            "matches": ["https://starlog.gg/en/leaderboard*"],
            "js": ["snitch.js"]
        }
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "19": "icons/icon-19.png",
        "38": "icons/icon-38.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png",
        "256": "icons/icon-256.png",
        "512": "icons/icon-512.png"
    }
}
